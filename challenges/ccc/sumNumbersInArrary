// Write a function called sumNumbers that takes in an array of numbers and returns the sum of those numbers using an arrow function. 

const sumNumbers = (arrayOfNums) => {
    return arrayOfNums.reduce((acc, curr) => acc + curr, 0)
}

// Example usage:
const numbersArray = [1, 2, 3, 4, 5];
const sum = sumNumbers(numbersArray);

console.log("Sum of numbers:", sum);